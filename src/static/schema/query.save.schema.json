{
  "$schema": "https://json-schema.org/draft-04/schema",
  "properties": {
    "cacheDurationSeconds": {
      "type": "integer",
      "description": "Cache duration in seconds"
    },
    "range": {
      "type": "string",
      "description": "A1 notation"
    },
    "spreadsheet": {
      "type": "string",
      "description": "The google sheets spreadsheet ID"
    },
    "useTimeFilter": {
      "type": "boolean",
      "description": "Use the query time range to filer values from the table"
    },
    "refId": {
      "type": "string"
    },
    "resultAssertions": {
      "properties": {
        "type": {
          "type": "string"
        },
        "typeVersion": {
          "items": {
            "type": "integer"
          },
          "type": "array",
          "maxItems": 2,
          "minItems": 2
        },
        "maxBytes": {
          "type": "integer"
        },
        "maxFrames": {
          "type": "integer"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "typeVersion"
      ]
    },
    "timeRange": {
      "properties": {
        "from": {
          "type": "string"
        },
        "to": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "from",
        "to"
      ]
    },
    "datasource": {
      "properties": {
        "type": {
          "type": "string"
        },
        "uid": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "type",
        "uid"
      ]
    },
    "datasourceId": {
      "type": "integer"
    },
    "queryType": {
      "type": "string"
    },
    "maxDataPoints": {
      "type": "integer"
    },
    "intervalMs": {
      "type": "number"
    },
    "hide": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "type": "object",
  "required": [
    "spreadsheet"
  ],
  "description": "Save model (the payload saved in dashboards and alerts)"
}